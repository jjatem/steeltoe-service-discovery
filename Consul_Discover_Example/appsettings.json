{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "Consul-Discovery-Example"
    }
  },
  "consul": {
    "host": "consul-server.consul.svc.cluster.local",
    "port": 8500,
    "discovery": {
      "enabled": true,
      "register": true,
      "port": "80",
      "ipAddress": "consul-discovery-webapi-service",
      "preferIpAddress": true
    }
  }
}
